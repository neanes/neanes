{
  "name": "neanes",
  "productName": "Neanes",
  "version": "0.4.104",
  "private": true,
  "description": "A Byzantine Chant Scorewriter",
  "author": "danielgarthur",
  "repository": {
    "type": "git",
    "url": "https://github.com/neanes/neanes.git"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "release": "vue-tsc --noEmit && vite build && electron-builder",
    "lint": "eslint && prettier --check \"./**/*.{ts,js,vue}\"",
    "lint:fix": "eslint --fix && prettier --write \"./**/*.{ts,js,vue}\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test": "vitest --run",
    "test:dev": "vitest",
    "coverage": "vitest run --coverage",
    "web:dev": "vite --mode web",
    "web:build": "vite build --mode web"
  },
  "main": "dist-electron/main/index.js",
  "dependencies": {
    "@ckeditor/ckeditor5-vue": "^7.3.0",
    "@ckpack/vue-color": "^1.6.0",
    "ckeditor5": "^45.2.1",
    "html-to-image": "^1.11.13",
    "i18next": "^25.3.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-pseudo": "^2.2.1",
    "i18next-vue": "^5.3.0",
    "image-size": "^2.0.2",
    "jszip": "^3.10.1",
    "mime-types": "^3.0.1",
    "mitt": "^3.0.1",
    "register-service-worker": "^1.7.2",
    "throttle-debounce": "^5.0.2",
    "tone": "^15.1.22",
    "uuid": "^11.1.0",
    "vue": "^3.5.17",
    "vue-draggable-resizable": "^3.0.0",
    "vue-facing-decorator": "^4.0.1",
    "vue-router": "^4.5.1",
    "vue3-observe-visibility": "^1.0.3",
    "vue3-tabs-chrome": "^0.3.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.0.9",
    "@types/throttle-debounce": "^5.0.2",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "electron": "^37.1.0",
    "electron-builder": "^26.0.17",
    "electron-updater": "^6.6.2",
    "eslint": "^9.30.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.2.0",
    "jest-matcher-deep-close-to": "^3.0.2",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^1.0.1",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.4",
    "vue-component-type-helpers": "^3.0.0",
    "vue-tsc": "^2.2.10"
  },
  "license": "GPL-3.0",
  "packageManager": "npm@11.4.2"
}
