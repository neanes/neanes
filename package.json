{
  "name": "neanes",
  "productName": "Neanes",
  "version": "0.4.50",
  "private": true,
  "description": "A Byzantine Chant Scorewriter",
  "author": "danielgarthur",
  "repository": {
    "type": "git",
    "url": "https://github.com/neanes/neanes.git"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "release": "vue-tsc --noEmit && vite build && electron-builder",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src && prettier --check \"./**/*.{ts,js,vue}\"",
    "lint:fix": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src --fix && prettier --write \"./**/*.{ts,js,vue}\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test": "vitest --run",
    "test:dev": "vitest",
    "web:dev": "vite --mode web",
    "web:build": "vite build --mode web"
  },
  "main": "dist-electron/main/index.js",
  "dependencies": {
    "@ckeditor/ckeditor5-vue": "^7.2.0",
    "@ckpack/vue-color": "^1.6.0",
    "ckeditor5": "^43.1.1",
    "html-to-image": "^1.11.11",
    "i18next": "^23.15.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-pseudo": "^2.2.1",
    "i18next-vue": "^5.0.0",
    "image-size": "^1.1.1",
    "jszip": "^3.10.1",
    "mime-types": "^2.1.35",
    "mitt": "^3.0.1",
    "register-service-worker": "^1.7.2",
    "throttle-debounce": "^5.0.2",
    "tone": "^15.0.4",
    "uuid": "^10.0.0",
    "vue": "^3.5.9",
    "vue-draggable-resizable": "^3.0.0",
    "vue-facing-decorator": "^3.0.4",
    "vue-router": "^4.4.5",
    "vue3-observe-visibility": "^1.0.1",
    "vue3-tabs-chrome": "^0.3.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.7.3",
    "@types/throttle-debounce": "^5.0.2",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "electron-updater": "^6.3.4",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.28.0",
    "jest-matcher-deep-close-to": "^3.0.2",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.20.5",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vitepress": "^1.3.4",
    "vitest": "^2.1.1",
    "vue-component-type-helpers": "^2.1.6",
    "vue-tsc": "^2.0.29"
  },
  "license": "GPL-3.0",
  "packageManager": "npm@10.7.0"
}
