{
  "name": "neanes",
  "productName": "Neanes",
  "version": "0.4.32",
  "private": true,
  "description": "A Byzantine Chant Scorewriter",
  "author": "danielgarthur",
  "repository": {
    "type": "git",
    "url": "https://github.com/neanes/neanes.git"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "release": "vue-tsc --noEmit && vite build && electron-builder",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src && prettier --check \"./**/*.{ts,js,vue}\"",
    "lint:fix": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src --fix && prettier --write \"./**/*.{ts,js,vue}\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test": "vitest --run",
    "test:dev": "vitest",
    "web:dev": "vite --mode web",
    "web:build": "vite build --mode web"
  },
  "main": "dist-electron/main/index.js",
  "dependencies": {
    "@ckeditor/ckeditor5-alignment": "41.4.2",
    "@ckeditor/ckeditor5-basic-styles": "41.4.2",
    "@ckeditor/ckeditor5-editor-inline": "41.4.2",
    "@ckeditor/ckeditor5-essentials": "41.4.2",
    "@ckeditor/ckeditor5-find-and-replace": "41.4.2",
    "@ckeditor/ckeditor5-font": "41.4.2",
    "@ckeditor/ckeditor5-image": "41.4.2",
    "@ckeditor/ckeditor5-indent": "41.4.2",
    "@ckeditor/ckeditor5-link": "41.4.2",
    "@ckeditor/ckeditor5-list": "41.4.2",
    "@ckeditor/ckeditor5-paragraph": "41.4.2",
    "@ckeditor/ckeditor5-paste-from-office": "41.4.2",
    "@ckeditor/ckeditor5-remove-format": "41.4.2",
    "@ckeditor/ckeditor5-select-all": "41.4.2",
    "@ckeditor/ckeditor5-table": "41.4.2",
    "@ckeditor/ckeditor5-theme-lark": "^41.4.2",
    "@ckeditor/ckeditor5-typing": "41.4.2",
    "@ckeditor/ckeditor5-undo": "41.4.2",
    "@ckeditor/ckeditor5-upload": "41.4.2",
    "@ckeditor/ckeditor5-vue": "^5.1.0",
    "@ckpack/vue-color": "^1.5.0",
    "html-to-image": "^1.11.11",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-pseudo": "^2.2.1",
    "i18next-vue": "^4.0.0",
    "image-size": "^1.1.1",
    "jszip": "^3.10.1",
    "mime-types": "^2.1.35",
    "mitt": "^3.0.1",
    "register-service-worker": "^1.7.2",
    "tone": "^15.0.4",
    "uuid": "^9.0.1",
    "vue": "^3.4.27",
    "vue-draggable-resizable": "^3.0.0",
    "vue-facing-decorator": "^3.0.4",
    "vue-router": "^4.3.2",
    "vue3-observe-visibility": "^1.0.1",
    "vue3-tabs-chrome": "^0.3.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.6",
    "@ckeditor/vite-plugin-ckeditor5": "^0.1.3",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.12.13",
    "@types/throttle-debounce": "^5.0.2",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "electron": "^29.4.1",
    "electron-builder": "^24.13.3",
    "electron-updater": "^6.1.8",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "jest-matcher-deep-close-to": "^3.0.2",
    "prettier": "^3.2.5",
    "throttle-debounce": "^5.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-vue-devtools": "^7.2.1",
    "vitepress": "^1.2.2",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19"
  },
  "overrides": {
    "vite-plugin-electron": {
      "electron": "$electron"
    }
  },
  "license": "GPL-3.0",
  "packageManager": "npm@10.2.3"
}
