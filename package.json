{
  "name": "neanes",
  "productName": "Neanes",
  "version": "0.4.5",
  "private": true,
  "description": "A Byzantine Chant Scorewriter",
  "author": "danielgarthur",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielgarthur/neanes.git"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "release": "vue-tsc --noEmit && vite build && electron-builder",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src && prettier --check \"./**/*.{ts,js,vue}\"",
    "lint:fix": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src --fix && prettier --write \"./**/*.{ts,js,vue}\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "icons:build": "electron-icon-builder --flatten --input=./icons/1024x1024.png --output=./build",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test": "vitest --run",
    "web:dev": "vite --mode web",
    "web:build": "vite build --mode web"
  },
  "main": "dist-electron/main/index.js",
  "dependencies": {
    "@ckpack/vue-color": "^1.5.0",
    "html-to-image": "^1.11.11",
    "image-size": "^1.0.2",
    "jszip": "^3.10.1",
    "mime-types": "^2.1.35",
    "mitt": "^3.0.0",
    "register-service-worker": "^1.7.2",
    "tone": "^14.8.49",
    "uuid": "^9.0.1",
    "vue": "^3.3.7",
    "vue-draggable-resizable": "^2.3.0",
    "vue-facing-decorator": "^3.0.4",
    "vue-router": "^4.2.5",
    "vue3-tabs-chrome": "^0.3.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.2",
    "@types/electron-devtools-installer": "^2.2.4",
    "@types/mime-types": "^2.1.3",
    "@types/node": "^20.8.10",
    "@types/throttle-debounce": "^5.0.1",
    "@types/uuid": "^9.0.6",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "electron": "^24.8.8",
    "electron-builder": "^24.6.4",
    "electron-devtools-installer": "^3.2.0",
    "electron-icon-builder": "^2.0.1",
    "electron-updater": "^6.1.4",
    "eslint": "^8.52.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "jest-matcher-deep-close-to": "^3.0.2",
    "prettier": "^3.0.3",
    "throttle-debounce": "^5.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-electron": "^0.14.1",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.16.6",
    "vitepress": "^1.0.0-rc.24",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "license": "GPL-3.0",
  "packageManager": "npm@9.8.1"
}
